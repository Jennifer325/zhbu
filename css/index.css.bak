	#nav #con ul li a:visited{background-color:#96c;}
	#nav #con ul li a:hover{background-color:green;color:#fff;}
	#noti{width:780px;height:27px;margin:auto;margin-top:20px;background:url("../images/notice.png");padding-top:13px;padding-left:220px;}
	#noti p{font-size:16px;}

/*个人修改页面 by WQ*/
