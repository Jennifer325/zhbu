/*index by WQ*/
    *{margin:0;padding:0;}
    #header{width:100%;height:70px;background:#9966cc;}
	a{text-decoration:none;font-size:12px;padding-top:5px;}
	a:link{color:#fff;}
	a:visited{color:#fff;}
	a:hover{color:purple;}
	#header #bar{width:600px;height:40px;margin:0px auto;padding-top:15px;}
	#header #bar a{text-decoration:none;font-size:12px;padding-top:5px;}
	#header #bar a:link{color:#fff;}
	#header #bar a:visited{color:#fff;}
	#header #bar a:hover{color:red;}
	#adv{width:100%;height:350px;background:#b4ddf9 url("../images/1.jpg") no-repeat center;}
    .title{width:1000px;height:40px;margin:20px auto;}
	#main{width:1000px;height:600px;margin:0 auto;}
	#main ul li{overflow:hidden;position:relative;width:270px;height:230px;border:2px solid #9966cc;list-style-type:none;float:left;margin:20px 15px 15px 30px;box-shadow:0px 0px 10px #000;padding:5px;}
	#main ul li p{font-size:14px;font-family:"微软黑雅";text-align:center;height:30px;line-height:30px;}
    #main ul li p.st{height:0px;width:100%;color:#fff;}
	#main ul li:hover p.st{height:205px;background:rgba(0,0,0,0.3);position:absolute;top:0px;left:0px;color:#fff;line-height:230px;text-align:center;font-weight:bold;transition:all 1s;}
	#main ul li img{width:270px;height:200px;}
	#main ul li a{text-decoration:none;}
	#main ul li a:link{color:#000;}
	#main ul li a:visited{color:#96c;}
	#main ul li a:hover{color:red;}
	.adv2{width:1000px;height:100px;background:#b4ddf9 url("../images/adv2.jpg");margin:3px auto;color:#000;font-size:16px;}
	.adv2 p{height:20px;font-family:"幼圆";text-align:center;padding:30px 10px;line-height:20px;}
	#footer{width:100%;height:60px;margin-top:30px;background:#9966cc;font-size:12px;font-family:"微软黑雅";text-align:center;padding-top:10px;color:white;}
	.clear{clear:both;}
	span{color:red;}
	#header #bar #welcome_notice{text-align:right;color:white;font-size:12px;float:right;}/*注册 登录*/

/*注册页面 by WQ*/
    #register{width:600px;height:420px;border:1px solid #9900cc;box-shadow:0 0 20px #660099;margin:100px auto;padding:15px;}
	#register p{color:gray;font-size:12px;}
	#register a{color:#950e14;}
    #register a:link{color:#950e14;}
	#register a:visited{color:#950e14;}
	#register a:hover{text-decoration:underline;font-weight:bold;}
	#register table{margin:auto;}
	#register table tr td{height:30px;width:100px;text-align:left;padding-bottom:3px;}
	#register table tr:hover{text-shadow:2px 2px 10px #7e32ca;}
	#register table tr td select.gender{height:23px;width:120px;}
	#register table tr td input.but1{width:100px;height:30px;background:url("../images/submit.png") no-repeat center;border:0;margin-left:60px;}
	#register table tr td input.but2{width:100px;height:30px;background:url("../images/reset.png") no-repeat center;border:0;margin-left:100px;}
	#register table tr td .info {font-size:10px;border:none; background-color:#fff;color:#F00;}

/*登录页面 by WQ*/
    #login{width:420px;height:225px;border:1px solid #9900cc;box-shadow:0 0 20px #660099;margin:100px auto;padding:15px;}
	#login table{margin:auto;}
	#login table tr td{height:30px;width:auto100px;height:30px;width:80px;}
	#login .notice{text-align:right;}
	#login img{padding:auto;}
	#login a{font-size:12px;}
	#login a:link{color:#9966cc;}
	#login a:visited{color:brown;}
	#login a:hover{color:green;}

/*个人信息页面 by WQ*/
    
	#adv_per{width:1000px;height:300px;margin:auto;margin-top:20px;border:6px solid #ddd;overflow:hidden;}
	#adv_per ul{width:3000px;}
    /*transition：过渡，缓冲时间*/
	#adv_per ul li{list-style-type:none;width:166px;height:300px;background:#000;float:left;border-left:1px solid #ddd;transition:all 1s;position:relative;box-shadow:-4px 0px 34px #000;}
	#adv_per ul li img{width:1000px;height:300px;}
	#adv_per ul:hover li{width:10px;}
	#adv_per ul li:hover{width:980px;}
	/*文字*/
	#adv_per ul li p{height:300px;width:166px;background:rgba(0,0,0,0.3);position:absolute;top:0px;left:0px;color:#000;line-height:300px;text-align:center;font-weight:bold;text-shadow:2px 2px 4px #fff;transition:all 1s;}
	#adv_per ul li p.p1{color:#fff;}
	#adv_per ul:hover li p{width:40px;}
	#adv_per ul li:hover p{width:980px;height:48px;}
    #adv_per ul li:hover p.p1{width:980px;height:48px;top:0px;left:0px;line-height:40px;}
    #adv_per ul li:hover p.p2{width:980px;height:48px;top:260px;left:0px;line-height:40px;}
	#icon{width:1000px;height:250px;background:#fff;margin:auto;}
	#icon #left{width:40%;height:240px;float:left;display:table-cell;vertical-align:middle;padding-top:10px;text-align:center;}
	#icon #left img{width:350px;height:200px;}
	#icon p{text-indent:6em;font-family:"方正舒体";text-align:left;padding-top:5px;}
	#icon #left ul li{width:200px;height:25px;font-size:12px;font-family:"方正舒体";list-style-type:none;}
	#icon #right{float:left;width:60%;height:250px;margin:auto;}
	#icon #right table{width:400px;height:200px;margin:auto;padding-top:50px;font-size:12px;}
	#icon #right table tr td{border-bottom:1px solid #ddd;}
	#icon #right table a{color:gray;text-decoration:underline;float:right;}
	#icon #right table a:hover{font-weight:bold;}

/*各页面导航栏样式 by WQ*/
    #nav{width:1000px;height:40px;background:#96c;margin:auto;}
	#nav #con{width:700px;margin:auto;height:40px;}
	#nav #con ul li{float:left;width:100px;height:35px;background:#96c;list-style-type:none;text-align:center;padding-top:5px;}
